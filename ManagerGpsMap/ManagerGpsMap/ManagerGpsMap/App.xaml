<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ManagerGpsMap.App">
    <Application.Resources>
        <ResourceDictionary>
            <Color x:Key="primaryColor">#FFB406</Color>
            <Style x:Key="indicator" TargetType="ActivityIndicator">
                <Setter Property="AbsoluteLayout.LayoutBounds" Value="0.5,0.5,50,50" />
                <Setter Property="AbsoluteLayout.LayoutFlags" Value="PositionProportional" />
                <Setter Property="Color" Value="#C2C3C9" />
            </Style>
        </ResourceDictionary>
    </Application.Resources>
</Application>